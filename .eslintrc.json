{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "rules": {
        "eol-last": 0,
        "object-shorthand": 0,
        "padded-blocks": 0,
        "no-unused-vars": 0,
        "arrow-body-style": 0,
        "import/prefer-default-export": 0,
        "import/no-named-default": 0,
        "class-methods-use-this": 0,
        "import/export": 0,
        "no-useless-constructor": 0,
        "no-empty-function": 0,
        "import/named": 0,
        "no-empty": 0,
        "semi": 0,
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "prefer-template": 0,
        "no-plusplus": 0,
        "prefer-destructuring": 0,
        "max-len": [
            "warn",
            {
                "code": 120
            }
        ]
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [
                    ".js"
                ]
            }
        }
    }
}